{
  "name": "package-json-rewrite",
  "version": "1.0.0",
  "description": "Simple wrapper to inject GitHub personal access tokens into package.json on npm/yarn install",
  "bin": {
    "package-json-rewrite": "./bin/package-json-rewrite.js"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:connectedcars/node-package-json-rewrite.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.0.1",
    "@types/jest": "^24.0.13",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.17.1",
    "jest": "^24.8.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testRegex": ".+\\.test\\.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/types/**"
    ],
    "moduleFileExtensions": [
      "js",
      "node"
    ]
  }
}
