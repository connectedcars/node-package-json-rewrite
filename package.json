{
  "name": "package-json-rewrite",
  "version": "1.0.0",
  "description": "Simple wrapper to inject GitHub personal access tokens into package.json on npm/yarn install",
  "bin": {
    "package-json-rewrite": "./bin/package-json-rewrite.js"
  },
  "main": "src/index.js",
  "scripts": {
    "export": "export",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:connectedcars/node-package-json-rewrite.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.3",
    "@types/jest": "^25.2.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "jest": "^26.0.1"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testRegex": ".+\\.test\\.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/types/**"
    ],
    "moduleFileExtensions": [
      "js",
      "node"
    ]
  }
}
